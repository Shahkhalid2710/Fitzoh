<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/spacing_10"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tvDay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_regular"
            android:text="@string/day"
            android:textColor="@color/black"
            android:textSize="@dimen/font_14" />

        <TextView
            android:id="@+id/tvDate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_regular"
            android:gravity="end"
            android:text="@string/date"
            android:textColor="@color/black"
            android:textSize="@dimen/font_14" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/spacing_10"
        android:background="@drawable/layout_result"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tvTime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_regular"
                android:text="@string/time"
                android:gravity="center"
                android:textColor="@color/bluee"
                android:textSize="@dimen/font_14" />
            <TextView
                android:id="@+id/tvResult"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_regular"
                android:text="@string/_result"
                android:gravity="center"
                android:textColor="@color/bluee"
                android:textSize="@dimen/font_14" />
            <TextView
                android:id="@+id/tvComment"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_regular"
                android:text="@string/comment"
                android:gravity="center"
                android:textColor="@color/bluee"
                android:textSize="@dimen/font_14" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/bluee"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:padding="@dimen/spacing_5"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tvTime1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_medium"
                android:text="@string/time"
                android:gravity="center"
                android:textColor="@color/textcolorgray"
                android:textSize="@dimen/font_13" />
            <TextView
                android:id="@+id/tvResult1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_medium"
                android:text="@string/_result"
                android:gravity="center"
                android:textColor="@color/textcolorgray"
                android:textSize="@dimen/font_13" />
            <TextView
                android:id="@+id/tvComment1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_medium"
                android:text="@string/comment"
                android:gravity="center"
                android:textColor="@color/textcolorgray"
                android:textSize="@dimen/font_13" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:padding="@dimen/spacing_5"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tvTime2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_medium"
                android:text="@string/time"
                android:gravity="center"
                android:textColor="@color/textcolorgray"
                android:textSize="@dimen/font_13" />
            <TextView
                android:id="@+id/tvResult2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_medium"
                android:text="@string/_result"
                android:gravity="center"
                android:textColor="@color/textcolorgray"
                android:textSize="@dimen/font_13" />
            <TextView
                android:id="@+id/tvComment2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_medium"
                android:text="@string/comment"
                android:gravity="center"
                android:textColor="@color/textcolorgray"
                android:textSize="@dimen/font_13" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:padding="@dimen/spacing_5"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tvTime3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_medium"
                android:text="@string/time"
                android:gravity="center"
                android:textColor="@color/textcolorgray"
                android:textSize="@dimen/font_13" />
            <TextView
                android:id="@+id/tvResult3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_medium"
                android:text="@string/_result"
                android:gravity="center"
                android:textColor="@color/textcolorgray"
                android:textSize="@dimen/font_13" />
            <TextView
                android:id="@+id/tvComment3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/poppins_medium"
                android:text="@string/comment"
                android:gravity="center"
                android:textColor="@color/textcolorgray"
                android:textSize="@dimen/font_13" />
        </LinearLayout>

    </LinearLayout>

</LinearLayout>